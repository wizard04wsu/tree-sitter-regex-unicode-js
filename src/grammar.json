{
  "name": "regex",
  "rules": {
    "regex": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "$pattern"
        },
        {
          "type": "SYMBOL",
          "name": "$disjunction"
        }
      ]
    },
    "$disjunction": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "$pattern"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "disjunction_delimiter"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "$pattern"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "$pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "SYMBOL",
                    "name": "$quantifier"
                  },
                  "named": true,
                  "value": "invalid"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "REPEAT1",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "$boundary_assertion"
                  },
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "$repeatable_symbol"
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "$quantifier"
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "$invalid_syntax_character"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "$quantifier"
          },
          "named": true,
          "value": "invalid"
        }
      ]
    },
    "$repeatable_symbol": {
